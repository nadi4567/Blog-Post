<%- include('partials/header') %>

<h1>Home</h1>
<p><%= startingContent %></p>

<% posts.forEach((items)=>{ %>
   
    <h1><%= items.title %></h1>
    <p id="new"><%= items.content.substring(0,100)+"..." %> 
        <a href="/posts/<%= items._id %>">Read More</a>
    </p>
     <% }) %>  


<%- include('partials/footer') %>